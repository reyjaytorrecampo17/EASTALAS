{
  "expo": {
    "name": "TALAS",
    "slug": "TALAS",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/talasIcon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "talas",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.betzalel.TALAS"
    },
    "web": {
      "favicon": "./assets/adaptive.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash-icon.png"
        }
      ],
      "react-native-edge-to-edge"
    ],
    "extra": {
      "eas": {
        "projectId": "0cd0ccae-b08f-415d-aac7-93fe97009e28"
      }
    },
    "owner": "betzalel"
  }
}
